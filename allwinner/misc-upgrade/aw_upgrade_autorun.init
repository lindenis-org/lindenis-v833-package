#Copyright (C) 2006-2011 OpenWrt.org

START=99

USE_PROCD=1
PROG=/sbin/aw_upgrade_log.sh
DEPEND=boot

start_service(){
    procd_open_instance
    procd_set_param command $PROG start
    procd_close_instance
}

stop_service(){
    procd_open_instance
    procd_set_param command $PROG stop
    procd_close_instance
}

restart(){
    stop "$@"
    start "$@"
}
