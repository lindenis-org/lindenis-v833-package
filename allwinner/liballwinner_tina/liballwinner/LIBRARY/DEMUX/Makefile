
all:
	make -C BASE/
	make -C STREAM/
	make -C PARSER/
	make -C MUXER/
clean:
	make clean -C BASE/
	make clean -C STREAM/
	make clean -C PARSER/
	make clean -C MUXER/

cleanall:
	make cleanall -C BASE/
	make cleanall -C STREAM/
	make cleanall -C PARSER/
	make cleanall -C MUXER/

.PHONY: all clean cleanall
